
{
  "rewrites": [
    { "source": "/auth/callback", "destination": "/" },
    { "source": "/auth/callback/:path*", "destination": "/" }
  ],
  "routes": [
    { "src": "/auth/callback", "dest": "/" },
    { "src": "/auth/callback/(.*)", "dest": "/" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
