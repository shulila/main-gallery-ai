{
  "manifest_version": 3,
  "name": "MainGallery.AI",
  "version": "1.1.0",
  "description": "Collect and organize your AI-generated images across platforms",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "notifications",
    "identity",
    "cookies"
  ],
  "host_permissions": [
    "https://*.midjourney.com/*",
    "https://*.leonardo.ai/*",
    "https://*.openai.com/*",
    "https://*.stability.ai/*",
    "https://*.dreamstudio.ai/*",
    "https://*.runwayml.com/*",
    "https://*.pika.art/*",
    "https://*.krea.ai/*",
    "https://*.fluxlabs.ai/*",
    "https://*.hailuoai.video/*",
    "https://*.app.ltx.studio/*",
    "https://*.firefly.adobe.com/*",
    "https://*.studio.d-id.com/*",
    "https://*.app.heygen.com/*",
    "https://*.preview.reve.art/*",
    "https://*.lexica.art/*",
    "https://*.creator.nightcafe.studio/*",
    "https://*.looka.com/*",
    "https://*.reroom.ai/*",
    "https://*.genmo.ai/*",
    "https://*.app.botika.io/*",
    "https://*.playground.com/*",
    "https://*.dream.ai/*",
    "https://*.app.pixverse.ai/*",
    "https://*.starryai.com/*",
    "https://*.fotor.com/*",
    "https://*.deviantart.com/*",
    "https://*.deepai.org/*",
    "https://*.app.elai.io/*",
    "https://*.app.rundiffusion.com/*",
    "https://*.neural.love/*",
    "https://*.vidu.com/*",
    "https://*.promeai.pro/*",
    "https://*.genspark.ai/*",
    "https://*.main-gallery-ai.lovable.app/*",
    "https://*.preview-main-gallery-ai.lovable.app/*",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.midjourney.com/*",
        "https://*.leonardo.ai/*",
        "https://*.openai.com/*",
        "https://*.stability.ai/*",
        "https://*.dreamstudio.ai/*",
        "https://*.runwayml.com/*",
        "https://*.pika.art/*",
        "https://*.krea.ai/*",
        "https://*.fluxlabs.ai/*",
        "https://*.hailuoai.video/*",
        "https://*.app.ltx.studio/*",
        "https://*.firefly.adobe.com/*",
        "https://*.studio.d-id.com/*",
        "https://*.app.heygen.com/*",
        "https://*.preview.reve.art/*",
        "https://*.lexica.art/*",
        "https://*.creator.nightcafe.studio/*",
        "https://*.looka.com/*",
        "https://*.reroom.ai/*",
        "https://*.genmo.ai/*",
        "https://*.app.botika.io/*",
        "https://*.playground.com/*",
        "https://*.dream.ai/*",
        "https://*.app.pixverse.ai/*",
        "https://*.starryai.com/*",
        "https://*.fotor.com/*",
        "https://*.deviantart.com/*",
        "https://*.deepai.org/*",
        "https://*.app.elai.io/*",
        "https://*.app.rundiffusion.com/*",
        "https://*.neural.love/*",
        "https://*.vidu.com/*",
        "https://*.promeai.pro/*",
        "https://*.genspark.ai/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["bridge.js", "content-injection.js", "utils/*.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "key": "",
  "oauth2": {
    "client_id": "648580197357-2v9sfcorca7060e4rdjr1904a4f1qa26.apps.googleusercontent.com",
    "scopes": [
      "openid",
      "email",
      "profile"
    ]
  }
}
